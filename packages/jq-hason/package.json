{
  "name": "jq-hason",
  "version": "1.8.1",
  "description": "WebAssembly build of jq for JavaScript environments - a lightweight JSON processor",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist/**/*",
    "src/wasm/*.js",
    "src/wasm/*.wasm",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "jq",
    "json",
    "wasm",
    "webassembly",
    "processor",
    "filter",
    "transform"
  ],
  "author": "Hason Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/hason.git",
    "directory": "packages/jq-hason"
  },
  "bugs": {
    "url": "https://github.com/your-org/hason/issues"
  },
  "homepage": "https://github.com/your-org/hason/tree/main/packages/jq-hason#readme",
  "devDependencies": {
    "tsdown": "^0.15.6",
    "typescript": "~5.9.3",
    "@types/node": "^24.6.2"
  },
  "engines": {
    "node": ">=18"
  }
}
